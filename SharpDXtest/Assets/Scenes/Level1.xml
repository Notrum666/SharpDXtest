<?xml version="1.0" encoding="utf-8" ?>
<Scene mainCamera="{Reference mainCamera}">
  <Assets>
    <Models>
      <Model path="cube.obj"/>
    </Models>
    <Textures>
      <Texture path="template.png"/>
    </Textures>
  </Assets>

  <GameObject>
    <Transform>
      <Transform.Position x="0" y="0" z="0"/>
      <Transform.Rotation/>
    </Transform>
    <Camera x.Name="mainCamera" resolution="1,7777" near="0,001" far="100" FOV="1,5"/>
    <Controller speed="5" cube="{Reference Cube}"/>
  </GameObject>

  <GameObject x.Name="Cube">
    <Transform>
      <Transform.Position y="0" z="5"/>
    </Transform>
    <Rigidbody Mass="1">
      <Rigidbody.Material Friction="1" Bounciness="1"/>
    </Rigidbody>
    <CubeCollider>
      <CubeCollider.Size x="2" y="2" z="2"/>
    </CubeCollider>
    <Mesh model="{Model cube}" texture="{Texture template}"/>
  </GameObject>

  <GameObject>
    <Transform>
      <Transform.Position y="0"/>
    </Transform>
    <Rigidbody Mass="1000">
      <Rigidbody.Material Friction="1" Bounciness="1"/>
    </Rigidbody>
    <CubeCollider>
      <CubeCollider.Size x="4" y="4" z="2"/>
    </CubeCollider>
    <Mesh model="{Model cube}" texture="{Texture template}"/>
  </GameObject>
  
  
</Scene>