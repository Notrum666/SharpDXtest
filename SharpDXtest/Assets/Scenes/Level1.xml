<?xml version="1.0" encoding="utf-8" ?>
<Scene mainCamera="{Reference mainCamera}">
  <Assets>
    <Models>
      <Model path="cube.obj"/>
    </Models>
    <Textures>
      <Texture path="template.png"/>
    </Textures>
  </Assets>

  <GameObject>
    <Transform>
      <Transform.localPosition x="5"/>
      <Transform.localRotation z="1"/>
    </Transform>
    <GameObject>
      <Camera x.Name="mainCamera" resolution="1,7777" near="0,001" far="100" FOV="1,5"/>
      <Controller speed="5" cube="{Reference Cube}"/>
    </GameObject>
  </GameObject>

  <GameObject x.Name="Cube">
    <Transform>
      <Transform.localPosition y="5"/>
    </Transform>
    <Rigidbody/>
    <Mesh model="{Model cube}" texture="{Texture template}"/>
  </GameObject>
  
  
</Scene>