<?xml version="1.0" encoding="utf-8" ?>
<Scene mainCamera="{Reference mainCamera}">
  <Assets>
    <Models>
      <Model path="cube.obj"/>
    </Models>
    <Textures>
      <Texture path="template.png"/>
    </Textures>
  </Assets>
  
  <GameObject>
    <Camera x.Name="mainCamera" resolution="1,7777" near="0,001" far="100" FOV="1,5"/>
    <Controller speed="5"/>
  </GameObject>

  <GameObject>
    <Transform>
      <Transform.position x="0" y="10" z="0"/>
    </Transform>
    <Mesh model="{Model cube}" texture="{Texture template}"/>    
  </GameObject>
  
  
</Scene>