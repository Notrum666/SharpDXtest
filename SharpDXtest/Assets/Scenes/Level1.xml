<?xml version="1.0" encoding="utf-8" ?>
<Scene MainCamera="{Reference cam}">
  <GameObject>
    <Transform>
      <Transform.position X="0" Y="0" Z="-3"/>
    </Transform>
    <Camera x.Name="cam" resolution="1,7777" near="0,001" far="100" FOV="1,5"/>
    <Controller speed="5"/>
  </GameObject>
  <GameObject>
    <Mesh model="{Model cube}" texture="{Texture template}"/>
  </GameObject>
  <GameObject>
    <Mesh model="{Model diamondwhite_dmesh}" texture="{Texture default_white}">
      <Mesh.material>
        <Material metallic="76,8">
          <Material.ambient R="0,0215" G="0,1745" B="0,0215" A="1"/>
          <Material.diffuse R="0,07568" G="0,61424" B="0,07568" A="1"/>
          <Material.specular R="0,633" G="0,727811" B="0,633" A="1"/>
        </Material>
      </Mesh.material>
    </Mesh>
  </GameObject>
  <GameObject>
    <Transform>
      <Transform.position Y="2"/>
    </Transform>
    <AmbientLight Brightness="0,1"/>
    <PointLight Brightness="0,7" Radius="20" Intensity="0">
      <PointLight.color>
        <Color4 R="1" G="1" B="1" A="1"/>
      </PointLight.color>
    </PointLight>
    <AutoFlyAround/>
  </GameObject>
</Scene>